<%- include header.ejs %>
<div class="container-fluid" style="margin-top:50px ">
    <p>Welcome to <%= title %></p>
    <div class="f">
        <form class="form-horizontal">
             <div class="form-group">
                <label for="username" class="col-sm-2 control-label" >用户名:</label>
                <div class="col-sm-4">
                    <input type="text" class="form-control" id="username" placeholder="必填 用户名">
                </div>
             </div>
            <div class="form-group">
                <label for="inputPassword" class="col-sm-2 control-label" >密码:</label>
                <div class="col-sm-4">
                    <input type="password" class="form-control" id="inputPassword" placeholder="必填 用户名">
                </div>
            </div>
            <div class="form-group">
                <label for="inputRePassword" class="col-sm-2 control-label" >重复密码:</label>
                <div class="col-sm-4">
                    <input type="password" class="form-control" id="inputRePassword" placeholder="必填 重复密码">
                </div>
            </div>
            <div class="form-group">
                <label for="inputEMail" class="col-sm-2 control-label" >Email:</label>
                <div class="col-sm-4">

                    <input type="email" class="form-control" id="inputEMail" placeholder="必填 Email">
                </div>
            </div>
            <div class="form-group">
                <label for="inputPhone" class="col-sm-2 control-label" >电话:</label>
                <div class="col-sm-4">
                    <input type="tel" class="form-control" id="inputPhone" placeholder="必填 电话">
                </div>
            </div>
            <div class="form-group">
                <label for="inputQQ" class="col-sm-2 control-label" >QQ:</label>
                <div class="col-sm-4">
                    <input type="text" class="form-control" id="inputQQ" placeholder="QQ">
                </div>
            </div>
            <div class="form-group">
                <label for="inputCompany" class="col-sm-2 control-label" >单位:</label>
                <div class="col-sm-4">

                    <input type="email" class="form-control" id="inputCompany" placeholder="单位">
                </div>
            </div>

        </form>
    </div>
</div>

<%- include footer.ejs %>

